" .gvimrc

" Turns on the tab bar always
" set showtabline=2

" Number of horizontal lines on the screen
set lines=68

" GUI Option to remove the Toolbar (T)
" set guioptions-=T

" Sets the font and size
set guifont=MonacoNewPowerline:h12
